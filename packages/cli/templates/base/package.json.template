{
  "name": "{{ projectName }}",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "serve": "bff serve",
    "build": "bff build"
  },
  "dependencies": {
  {{#each dependencies }}
    "{{ this.name }}": "{{ this.version }}"{{#unless @last}},{{/unless}}
  {{/each}}
  },
  "devDependencies": {
  {{#each devDependencies}}
    "{{ this.name }}": "{{ this.version }}"{{#unless @last}},{{/unless}}
  {{/each}}
  }
}
